{
    "name": "rinodrummer/squirephp-it",
    "description": "A library containing the Italian translations of Squire models.",
    "keywords": [
        "squire"
    ],
    "license": "MIT",
    "homepage": "https://github.com/squirephp",
    "support": {
        "issues": "https://github.com/squirephp/squire/issues",
        "source": "https://github.com/squirephp/squire"
    },
    "authors": [
        {
            "name": "Dan Harrin",
            "email": "dan@danharrin.com"
        }
    ],
    "require": {
        "illuminate/support": "^8.0|^9.0|^10.0",
        "php": "^8.0",
        "squirephp/continents": "self.verion",
        "squirephp/countries": "self.version",
        "squirephp/repository": "3.4.4"
    },
    "autoload": {
        "psr-4": {
            "Squire\\": [
                "packages/continents-it/src",
                "packages/countries-it/src"
            ]
        }
    },
    "require-dev": {
        "symplify/monorepo-builder": "^9.4.21"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Squire\\ContinentsItServiceProvider",
                "Squire\\CountriesItServiceProvider"
            ]
        }
    },
    "replace": {
        "rinodrummer/squirephp-continents-it": "self.version",
        "rinodrummer/squirephp-countries-it": "self.version"
    },
    "prefer-stable": true
}
